{
  "name": "cdk-step",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elthrasher/cdk-step-functions-example"
  },
  "bin": {
    "cdk-step": "build/bin/cdk-step.js"
  },
  "scripts": {
    "clean": "rimraf cdk.out",
    "deploy": "npx cdk deploy --require-approval=never",
    "destroy": "npx cdk destroy --force",
    "lint": "eslint --ext .ts .",
    "pretest": "npm run lint",
    "synth": "npx cdk synth --no-staging > template.yaml",
    "test": "jest",
    "watch": "cdk watch --hotswap-fallback"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.29.0",
    "aws-cdk": "^2.1006.0",
    "esbuild": "^0.25.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.186.0",
    "constructs": "^10.4.2"
  }
}
